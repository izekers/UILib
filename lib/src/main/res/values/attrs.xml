<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialImageView">
        <attr name="material_mode">
            <enum name="circle" value="0"/>
            <enum name="rounded_corners" value="1"/>
            <enum name="normal" value="-1"/>
        </attr>
        <attr name="round_size" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ColorButton">
        <attr name="normal_color" format="color"/>
        <attr name="press_color" format="color"/>
        <attr name="radius" format="dimension"/>
        <attr name="border_color" format="color"/>
        <attr name="border_width" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="GifImageView">

    </declare-styleable>
    <declare-styleable name="BottomNavigationBar">

    </declare-styleable>


    <declare-styleable name="FlowLayout">
        <attr name="flow_gravity">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>
</resources>